{
	"name": "flowbite-svelte-starter",
	"version": "0.1.3",
	"description": "Flowbite-Svelte starter kit",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://blog.codewithshin.com"
	},
	"bugs": "https://github.com/shinokada/flowbite-svelte-starter/issues",
	"homepage": "https://github.com/shinokada/flowbite-svelte-starter",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"classnames": "^2.3.2",
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/kit": "1.7.2",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^1.6.3",
		"flowbite-svelte": "^0.44.4",
		"flowbite-svelte-blocks": "^0.1.6",
		"flowbite-svelte-icons": "^0.3.6",
		"@lottiefiles/svelte-lottie-player": "^0.3.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-carousel": "^1.0.25",
		"svelte-check": "^2.10.3",
		"svelte-heros": "^2.6.0",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "~4.7.4",
		"vite": "^4.1.2",
		"vitest": "^0.28.5"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"sveltekit",
		"flowbite",
		"flowbite-svelte",
		"tailwindcss"
	],
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shinokada/flowbite-svelte-starter.git"
	}
}

